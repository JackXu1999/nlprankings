    pages = "205--209",
    abstract = "We perform the LSTM and BiLSTM model for the emotion intensity prediction. We only join the third subtask in Task 1:Affect in Tweets. Our system rank 6th among all the teams.",
}
@inproceedings{abdou-etal-2018-affecthor,
    title = "{A}ffec{T}hor at {S}em{E}val-2018 Task 1: A cross-linguistic approach to sentiment intensity quantification in tweets",
    author = "Abdou, Mostafa  and
